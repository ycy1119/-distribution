<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

    <context id="mysqlgenerator" targetRuntime="MyBatis3Simple">

        <commentGenerator>
            <property name="suppressAllComments" value="true" />
        </commentGenerator>

        <jdbcConnection driverClass="org.mariadb.jdbc.Driver"
                        connectionURL="jdbc:mariadb://localhost/lagou"
                        userId="root"
                        password="rootroot">
        </jdbcConnection>

        <javaModelGenerator targetPackage="test.entity" targetProject="E:\二学年\idea_wed\ssm_wuliu\src\main\java">
            <property name="trimStrings" value="true" />
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="mappers"  targetProject="E:\二学年\idea_wed\ssm_wuliu\src\main\resources">
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER" targetPackage="test.dao"  targetProject="E:\二学年\idea_wed\ssm_wuliu\src\main\java">
        </javaClientGenerator>

        <table tableName="wl_%">
            <property name="useActualColumnNames" value="true"/>
        </table>
    </context>
</generatorConfiguration>